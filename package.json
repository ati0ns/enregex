{
    "name": "enregex",
    "version": "1.8.4",
    "description": "To make regex easier, faster, stronger: so better.",
    "main": "./src/index.js",
    "types": "./typings/index.d.ts",
    "exports": {
        ".": [
            {
                "require": "./src/index.js",
                "import": "./esm/index.mjs"
            },
            "./src/index.js"
        ],
        "./esm": "./esm/index.mjs"
    },
    "scripts": {
        "test": "npm run lint && node ./src/tests.js",
        "lint": "eslint .",
        "prepublishOnly": "npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ati0ns/enregex.git"
    },
    "keywords": [
        "regex",
        "js",
        "javascript",
        "regexp"
    ],
    "author": "ations <brigadecopyright@gmail.com> (https://github.com/ati0ns)",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/ati0ns/enregex/issues"
    },
    "homepage": "https://github.com/ati0ns/enregex#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "eslint": "^7.24.0"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "eslintConfig": {
        "env": {
            "node": true,
            "es2021": true
        },
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "rules": {
            "camelcase": [
                "error",
                {
                    "properties": "always",
                    "ignoreDestructuring": true,
                    "ignoreGlobals": true
                }
            ],
            "eol-last": "error",
            "func-name-matching": "error",
            "getter-return": "error",
            "global-require": "error",
            "no-undef": "off",
            "quotes": "off",
            "indent": "error",
            "key-spacing": "error",
            "lines-between-class-members": "error"
        }
    },
    "readme": "./README.md"
}
